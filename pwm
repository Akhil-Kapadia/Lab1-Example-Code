S_00000000008c6130 .scope module, "tb_pwm" "tb_pwm" 2 16;
 .timescale -9 -12;
v00000000008a2aa0_0 .var "clk", 0 0;
v00000000008a2b40_0 .var "duty", 7 0;
v00000000008a2be0_0 .net "pulse", 0 0, v00000000008a2a00_0;  1 drivers
S_00000000008c62c0 .scope module, "wave1" "pwm" 2 26, 3 17 0, S_00000000008c6130;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 8 "duty";
    .port_info 2 /OUTPUT 1 "pulse";
P_00000000008a2fc0 .param/l "PERIOD" 0 3 17, +C4<00000000000000000000000011111111>;
P_00000000008a2ff8 .param/l "SIZE" 0 3 17, +C4<00000000000000000000000000001000>;
v00000000008a3100_0 .net "clk", 0 0, v00000000008a2aa0_0;  1 drivers
v00000000001cdc00_0 .var "count", 7 0;
v00000000008a3320_0 .net "duty", 7 0, v00000000008a2b40_0;  1 drivers
v00000000008a2a00_0 .var "pulse", 0 0;
E_00000000001ca950 .event posedge, v00000000008a3100_0;
    .scope S_00000000008c62c0;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v00000000001cdc00_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v00000000008a2a00_0, 0;
    %end;
    .thread T_0;
    .scope S_00000000008c62c0;
T_1 ;
    %wait E_00000000001ca950;
    %load/vec4 v00000000001cdc00_0;
    %pad/u 32;
    %cmpi/u 255, 0, 32;
    %flag_mov 8, 5;
    %jmp/0 T_1.0, 8;
    %load/vec4 v00000000001cdc00_0;
    %addi 1, 0, 8;
    %jmp/1 T_1.1, 8;
T_1.0 ; End of true expr.
    %pushi/vec4 0, 0, 8;
    %jmp/0 T_1.1, 8;
 ; End of false expr.
    %blend;
T_1.1;
    %assign/vec4 v00000000001cdc00_0, 0;
    %load/vec4 v00000000001cdc00_0;
    %load/vec4 v00000000008a3320_0;
    %cmp/u;
    %flag_mov 8, 5;
    %jmp/0 T_1.2, 8;
    %pushi/vec4 1, 0, 2;
    %jmp/1 T_1.3, 8;
T_1.2 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_1.3, 8;
 ; End of false expr.
    %blend;
T_1.3;
    %pad/s 1;
    %assign/vec4 v00000000008a2a00_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_00000000008c6130;
T_2 ;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v00000000008a2b40_0, 0, 8;
    %end;
    .thread T_2;
    .scope S_00000000008c6130;
T_3 ;
    %vpi_call 2 34 "$dumpfile", "tb_pwm.vcd" {0 0 0};
    %vpi_call 2 35 "$dumpvars", 32'sb00000000000000000000000000000000, S_00000000008c6130 {0 0 0};
    %vpi_call 2 36 "$display", "Begin testbench logging\012\012" {0 0 0};
    %end;
    .thread T_3;
    .scope S_00000000008c6130;
T_4 ;
    %vpi_call 2 42 "$display", "The duty is: %d", v00000000008a2b40_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000008a2aa0_0, 0, 1;
    %delay 2560000, 0;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v00000000008a2b40_0, 0, 8;
    %delay 10240000, 0;
    %pushi/vec4 192, 0, 8;
    %store/vec4 v00000000008a2b40_0, 0, 8;
    %delay 10240000, 0;
    %vpi_call 2 50 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_00000000008c6130;
T_5 ;
    %delay 5000, 0;
    %load/vec4 v00000000008a2aa0_0;
    %inv;
    %store/vec4 v00000000008a2aa0_0, 0, 1;
    %jmp T_5;
    .thread T_5;
